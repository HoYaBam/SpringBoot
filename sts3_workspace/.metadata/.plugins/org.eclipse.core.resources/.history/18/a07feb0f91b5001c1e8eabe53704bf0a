package com.mcs.total.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.mcs.total.domain.Skintest;
import com.mcs.total.repository.SkinRepository;

@Service
public class SkinService {
	@Autowired
	private SkinRepository skinRepo;
	
	public List<Skintest> skintest() {
		int DRPT = 0;
		int DRNT = 0;
		int DSPT = 0;
		int DSNT = 0;
		int DRPW = 0;
		int DRNW = 0;
		int DSPW = 0;
		int DSNW = 0;
		int ORPT = 0;
		int ORNT = 0;
		int OSPT = 0;
		int OSNT = 0;
		int ORPW = 0;
		int ORNW = 0;
		int OSPW = 0;
		int OSNW = 0;
		List<Skintest> skintest = skinRepo.findAll();
		for (int i = 0; i < skintest.size(); i++) {
			Skintest skin = skintest.get(i);
			if (skin.getBaumann().equals("DRPT")){
				DRPT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DSPT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			else if (skin.getBaumann().equals("DRNT")){
				DRNT += 1;
			}
			  System.out.println(skin.getBaumann());
		}
		return skinRepo.findAll();
	}
	
}
